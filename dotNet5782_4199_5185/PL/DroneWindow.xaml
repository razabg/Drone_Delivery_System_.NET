<Window x:Class="PL.DroneWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PL"
        mc:Ignorable="d"
        Title="Window1" Height="439" Width="438">
    <Grid Margin="0,-9,0,3">
        <TextBox x:Name="IDfill" HorizontalAlignment="Left" Margin="134,38,0,0" Text="ID" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" Height="24" TextChanged="IDfill_TextChanged"/>
        <ComboBox x:Name="choose_model" HorizontalAlignment="Left" Margin="134,85,0,0" VerticalAlignment="Top" Width="120" RenderTransformOrigin="-0.167,0.73" SelectionChanged="choose_model_SelectionChanged"/>
        <ComboBox x:Name="StationForCharge" HorizontalAlignment="Left" Margin="134,176,0,0" VerticalAlignment="Top" Width="120" SelectionChanged="StationForCharge_SelectionChanged"/>
        <Label Content="Insert ID" HorizontalAlignment="Left" Margin="33,38,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.237,1.119" Width="65"/>
        <Label Content="Choose Model" HorizontalAlignment="Left" Margin="22,81,0,0" VerticalAlignment="Top"/>
        <Label Content="Max Weight" HorizontalAlignment="Left" Margin="24,127,0,0" VerticalAlignment="Top"/>
        <Label Content="Station for Charge" HorizontalAlignment="Left" Margin="12,174,0,0" VerticalAlignment="Top"/>
        <ComboBox x:Name="MaxWeight" HorizontalAlignment="Left" Margin="134,127,0,0" VerticalAlignment="Top" Width="120" SelectionChanged="MaxWeight_SelectionChanged"/>
        <Button x:Name="Close" Content="Close" HorizontalAlignment="Left" Margin="332,382,0,0" VerticalAlignment="Top" Width="81" Click="Close_Click"/>
        <Button x:Name="add_drone" Content="Add Drone" HorizontalAlignment="Left" Margin="150,233,0,0" VerticalAlignment="Top" Width="88"/>

    </Grid>
</Window>
